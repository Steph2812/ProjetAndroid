<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ContactActivity">

    <TextView
        android:id="@+id/contact"
        android:layout_width="336dp"
        android:layout_height="43dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="96dp"
        android:text="@string/contact_Contact"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.355"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/contactDe"
        android:layout_width="142dp"
        android:layout_height="27dp"
        android:layout_marginTop="72dp"
        android:text="@string/contact_de"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toEndOf="@+id/imageContact"
        app:layout_constraintTop_toBottomOf="@+id/contact" />

    <ImageView
        android:id="@+id/imageContact"
        android:layout_width="93dp"
        android:layout_height="56dp"
        android:layout_marginTop="60dp"
        app:layout_constraintStart_toStartOf="@+id/contact"
        app:layout_constraintTop_toBottomOf="@+id/contact"
        app:srcCompat="@android:drawable/ic_menu_camera" />

    <TextView
        android:id="@+id/NomContact"
        android:layout_width="294dp"
        android:layout_height="32dp"
        android:layout_marginTop="96dp"
        android:text="@string/contact_Nom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contactDe" />
</androidx.constraintlayout.widget.ConstraintLayout>